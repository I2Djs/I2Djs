{
  "name": "i2djs",
  "version": "4.4.3",
  "description": "Integrated-2d, framework for 2D graphics rendering for SVG/Canvas/WebGl context",
  "type": "module",
  "module": "./src/main.js",
  "main": "./dist/i2d.esm.js",
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "rollup -wm -c rollup.config.js",
    "build": "rm -rf ./dist/ && rollup -c rollup.config.js",
    "lint": "eslint src/**",
    "lint-fix": "eslint --cache --fix src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/I2Djs/I2D.git"
  },
  "files": [
    "dist",
    "src",
    "examples/canvas",
    "examples/svg",
    "examples/webGL",
    "examples/pdf"
  ],
  "keywords": [
    "SVG",
    "Canvas",
    "DataJoin",
    "Data Visualization",
    "Gaming",
    "Visualization",
    "2D Graphics",
    "Graphics",
    "Animation",
    "Virtual Dom",
    "Bitmap",
    "Scalable Vector Graphics",
    "pixel",
    "Rendering",
    "Integrated 2D",
    "pdf"
  ],
  "author": "Narayana Swamy",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/i2djs/I2D/issues"
  },
  "homepage": "https://github.com/i2djs/I2D#readme",
  "dependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "blob-stream-i2d": "^1.1.0",
    "stackblur-canvas": "^2.6.0"
  },
  "resolutions": {
    "minimist": "^1.2.6",
    "terser": "^5.14.2"
  },
  "devDependencies": {
    "earcut": "^2.2.4",
    "flubber": "^0.4.2",
    "pdfkit": "^0.13.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.57.0",
    "rollup": "^4.12.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0"
  }
}
