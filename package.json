{
  "name": "i2djs",
  "version": "5.3.0",
  "description": "Integrated-2d, framework for 2D graphics rendering for SVG/Canvas/WebGl context",
  "type": "module",
  "module": "./dist/i2d.esm.js",
  "main": "./dist/i2d.cjs",
  "sideEffects": false,
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "rollup -wm -c rollup.config.js",
    "build": "rm -rf ./dist/ && rollup -c rollup.config.js",
    "lint": "eslint src/**",
    "lint-fix": "eslint --cache --fix src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/I2Djs/I2D.git"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "i2d",
    "i2djs",
    "javascript",
    "SVG",
    "canvas",
    "webgl",
    "pdf",
    "DataJoin",
    "Data Visualization",
    "Visualization",
    "2d",
    "Graphics",
    "Animation",
    "Bitmap",
    "Scalable Vector Graphics",
    "pixel",
    "Rendering",
    "Integrated 2D"
  ],
  "author": "Narayana Swamy",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/i2djs/I2D/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/i2djs/I2D#readme",
  "resolutions": {},
  "devDependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "assert": "^2.1.0",
    "blob-stream-i2d": "^1.1.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "6.0.3",
    "earcut": "^2.2.4",
    "eslint": "^8.57.0",
    "events": "^3.3.0",
    "flubber": "^0.4.2",
    "pdfkit": "^0.14.0",
    "process": "^0.11.10",
    "rollup": "^4.12.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "stackblur-canvas": "^2.6.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5"
  },
  "jspm": {
    "files": [
      "package.json",
      "LICENSE",
      "README.md",
      "dist/i2d.cjs",
      "dist/i2d.esm.js",
      "dist/i2d.esm.js",
      "dist/dependencies-bundle-esm.js"
